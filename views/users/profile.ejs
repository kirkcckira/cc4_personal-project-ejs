<%- include('../partials/header')%>
<div class="container">
    <h3>Edit your profile</h3>
    <form method="POST" action="#">
        <div class="form-group">
            <input class="form-control" type="password" id="password" name="password" placeholder="password"></input>
        </div>
        <div class="form-group">
            <input class="form-control" type="text" id="name" name="name" placeholder="name"></input>
        </div>
        <div class="form-group">
            <textarea class="form-control" row="10" col="50" id="description" name="description"
                placeholder="Short description about yourself."></textarea>
        </div>
        <input type="submit" value="Submit">
    </form>
</div>
<%- include('../partials/footer')%>